{
  "expo": {
    "name": "Do This",
    "slug": "do-this",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Done.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Done.png",
        "backgroundColor": "#175d84"
      },
      "package": "com.russellgn.dothis"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Done.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Done.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#175d84"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f739ab76-f528-4ead-a660-8a31d830835d"
      }
    },
    "owner": "russellgn"
  }
}
